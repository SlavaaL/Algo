
#include <stdio.h>
#include <math.h>


int main (void){

    double a, V, n;
    printf ("Сколько раз производить вычисление объема правильного тетрэдра?\n");
    scanf("%lf",&n);
    for (int i = 0; i < n; i++){
        printf ("Ребро тетраэдра a :");
        scanf("%lf", &a);
        V = (pow(a, 3) * sqrt(2)) / 12;
        printf ("Объем правильного тетраэдра V = %lf \n:", V);
        
    }
return 0;

}
